<html>
  <head>
    <title><%= title %></title>
    <%= render :file => 'layouts/_stylesheets.html.erb' %>
    
  </head>
  <body>
    <div class="container">
      <%= render :file => 'layouts/_header.html.erb' %>

      <div id="content" class="round">
        <%= yield %>
      </div>
      <%= render :file => 'layouts/_footer.html.erb' %>

      <div id="debug" class="round">
        <%= debug(params) if Rails.env.development? %>
      </div>
    </div>
  </body>
</html>
